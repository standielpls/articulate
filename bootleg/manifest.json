{
  "name": "BOOTLEG",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Bootleg stuff right here.",
  "icons": {
    "16": "icons/head-16.png",
    "128": "icons/head-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": {
      "19": "icons/head-16.png",
      "38": "icons/head-32.png"
    },
    "default_title": "Notes Taking Ext"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "https://us-central1-lancelot-274021.cloudfunctions.net/"
  ],
  "background": {
    "page": "popup.html"
  },
  "web_accessible_resources": [
    "styles/popup.css"
  ]
}
