{
  "name": "Notes Taking",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A Notes Taking Extension",
  "icons": {
    "16": "icons/head-16.png",
    "128": "icons/head-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab"
  ],
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
      "all_frames": true,
      "js": ["./scripts/contentscript.js"]
  }],
  "browser_action": {
    "default_icon": {
      "19": "icons/head-16.png",
      "38": "icons/head-32.png"
    },
    "default_title": "Notes Taking Ext",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "styles/popup.css",
    "*.ttf"
  ]
}
